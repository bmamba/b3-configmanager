<?xml version='1.0'?>

<configuration>
	<settings name="server">
		<set name="punkbuster">
			<input type="radiobutton">
				<values name="on" value="on"/>
				<values name="off" value ="off"/>
			</input>
		</set>
	</settings>
	<settings name="autodoc">
